<div class="alignleft">
  <% if (config.author){ %>
    <p>
    &copy; <%= new Date().getFullYear() %> <%= config.author %>
    <% } else { %>
    &copy; <%= new Date().getFullYear() %> <%= config.title %>
    </p>
  <% } %>
  <% if (config.powered_by) { %>
    <p>
      <ul>
        <li>Powered by:<li>
        <% for (var i in config.powered_by) { %>
          <li><a href="<%- config.powered_by[i] %>" target="_blank"><%= i %></a></li>
        <% } %>
      </ul>
    </p>
  <% } %>
</div>
<div class="clearfix"></div>
