<nav id="pagination">
  <% if (page.prev){ %>
    <a href="<%- config.root %><%- page.prev_link %>" class="alignleft prev"><%= __('page', page.prev) %><%= __('page_total', page.total) %></a>
  <% } %>
  <% if (page.next){ %>
    <a href="<%- config.root %><%- page.next_link %>" class="alignright next"><%= __('page', page.next) %><%= __('page_total', page.total) %></a>
  <% } %>
  <div class="clearfix"></div>
</nav>
